{
    "dart.flutterSdkPath": "base_app/.fvm/flutter_sdk",
    // Remove .fvm files from search
    "search.exclude": {
        "**/.fvm": true
    },
    // Remove from file watching
    "files.watcherExclude": {
        "**/.fvm": true
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "workbench.colorCustomizations": {
        "statusBar.debuggingBackground": "#6A5AFF"
    }
}